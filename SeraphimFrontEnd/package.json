{
  "name": "seraphim-front-end",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "ng-aot-server": "ng serve --aot --host 0.0.0.0 --port 4201",
    "build": "ng build && yarn tsc server.ts",
    "build-exe": "pkg . --target host",
    "docker-build": "docker build --rm -f Dockerfile -t terraseraph/seraphimrootserverfrontend:latest .",
    "docker-push": "docker push terraseraph/seraphimrootserverfrontend:latest",
    "docker-pull": "docker pull terraseraph/seraphimrootserverfrontend:latest",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "bin": "server.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.3",
